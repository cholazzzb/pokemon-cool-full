{
  "name": "pokemon_cool",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "postinstall": "[ -z \"$HUSKY\" ] && husky install .husky || echo \"ignore husky\" && pnpm compile-script && pnpm graphql-codegen",
    "prepare": "panda codegen",
    "dev": "NODE_ENV=development pnpm collect-assets && next dev",
    "build": "pnpm collect-assets && next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --ext ts --ext tsx",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "type-check": "tsc -p tsconfig.json",
    "test": "vitest",
    "test:preview": "vitest-preview",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e-debug": "PWDEBUG=1 playwright test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "compile-script": "tsc scripts/collect-assets.ts --outDir .temp",
    "collect-assets": "node .temp/collect-assets.js"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "pnpm lint",
      "pnpm format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.10.4",
    "@stitches/react": "^1.2.8",
    "@tanstack/react-query": "^4.36.1",
    "file-loader": "^6.2.0",
    "framer-motion": "^9.1.7",
    "graphql": "^15.8.0",
    "graphql-request": "^5.2.0",
    "next": "^14.2.23",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.8",
    "react-window-infinite-loader": "^1.0.9",
    "url-loader": "^4.1.1",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-brands-svg-icons": "^6.5.2",
    "@fortawesome/free-regular-svg-icons": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^2.1.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@pandacss/dev": "^0.39.2",
    "@playwright/test": "^1.44.1",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "babel-loader": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.23",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-storybook": "^0.5.13",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^23.2.0",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "runtypes": "^6.7.0",
    "storybook-addon-next": "^1.8.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0",
    "vitest-preview": "^0.0.1"
  },
  "engines": {
    "node": ">=20.0.0 <21.7.2"
  }
}
