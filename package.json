{
  "name": "pokemon_cool",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "postinstall": "[ -z \"$HUSKY\" ] && husky install .husky || echo \"ignore husky\" && yarn compile-script && yarn graphql-codegen",
    "dev": "NODE_ENV=development yarn collect-assets && next dev",
    "build": "yarn collect-assets && next build",
    "start": "next start",
    "lint": "eslint . --max-warnings 0 --ext ts --ext tsx",
    "format": "prettier --write '**/*.{ts,tsx}'",
    "type-check": "tsc --pretty --noEmit",
    "test": "vitest",
    "test:preview": "vitest-preview",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e-debug": "PWDEBUG=1 playwright test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "compile-script": "tsc scripts/collect-assets.ts --outDir .temp",
    "collect-assets": "node .temp/collect-assets.js"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@stitches/react": "^1.2.8",
    "@tanstack/react-query": "^4.28.0",
    "file-loader": "^6.2.0",
    "framer-motion": "^9.0.4",
    "graphql": "^15.6.0",
    "graphql-request": "^5.1.0",
    "next": "^14.2.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.6",
    "react-window-infinite-loader": "^1.0.8",
    "url-loader": "^4.1.1",
    "zustand": "^3.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@fortawesome/fontawesome-svg-core": "^6.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.3.0",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@graphql-codegen/cli": "^3.0.0",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@playwright/test": "^1.22.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@tanstack/react-query-devtools": "^4.28.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/fs-extra": "^11.0.1",
    "@types/react": "^18.0.28",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/react-window-infinite-loader": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "babel-loader": "^8.2.5",
    "eslint": "^8.36.0",
    "eslint-config-next": "^14.2.23",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.13",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^23.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react-test-renderer": "^17.0.2",
    "runtypes": "^6.5.1",
    "storybook-addon-next": "^1.6.7",
    "typescript": "^5.0.2",
    "vitest": "^1.0.4",
    "vitest-preview": "^0.0.1"
  },
  "packageManager": "yarn@1.22.19"
}
