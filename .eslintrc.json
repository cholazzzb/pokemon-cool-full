{
  "root": true,
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "rules": {
    "@typescript-eslint/ban-ts-comment": "off",
    "no-console": "warn",
    "eqeqeq": ["error", "always"],
    "react-hooks/exhaustive-deps": "off",
    "react/display-name": "off",
    "no-underscore-dangle": "off"
  }
}
